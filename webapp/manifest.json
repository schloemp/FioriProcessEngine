{
	"_version": "1.1.0",
	"sap.app": {
		"_version": "1.1.0",
		"id": "fpe",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"sourceTemplate": {
			"id": "ui5template.basicSAPUI5ApplicationProject",
			"version": "1.32.0"
		}
	},
	"sap.ui": {
		"_version": "1.1.0",
		"technology": "UI5",
		"icons": {
			"icon": "",
			"favIcon": "",
			"phone": "",
			"phone@2": "",
			"tablet": "",
			"tablet@2": ""
		},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		},
		"supportedThemes": ["sap_hcb", "sap_bluecrystal"]
	},
	"sap.ui5": {
		"_version": "1.1.0",
		"rootView": {
			"viewName": "fpe.view.Home",
			"type": "XML"
		},
		"dependencies": {
			"minUI5Version": "1.30.0",
			"libs": {
				"sap.ui.core": {},
				"sap.m": {},
				"sap.ui.layout": {}
			}
		},
		"config": {
			"processConfigLocal": "/Configuration.json"
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "fpe.i18n.i18n"
				}
			}
		},
		"resources": {
			"css": [{
				"uri": "css/style.css"
			}]
		},
		"routing": {
			"targets": {
				"Launcher": {
					"viewType": "XML",
					"transition": "fade",
					"clearAggregation": true,
					"viewName": "Launcher"
				},
				"ConfigSteps": {
					"viewType": "XML",
					"transition": "fade",
					"clearAggregation": true,
					"viewName": "PEConfigurationSteps"
				},
				"ConfigProcesses": {
					"viewType": "XML",
					"transition": "fade",
					"clearAggregation": "true",
					"viewName": "PEConfigurationProcesses"
				},
				"Home": {
					"viewType": "XML",
					"transition": "fade",
					"clearAggregation": "true",
					"viewName": "HomeChoose"
				}
			},
			"routes": [{
				"name": "Launcher",
				"pattern": "Launcher",
				"greedy": false,
				"target": ["Launcher"]
			}, {
				"name": "ConfigSteps",
				"pattern": "ConfigSteps",
				"greedy": false,
				"target": ["ConfigSteps"]
			}, {
				"name": "ConfigProcesses",
				"pattern": "ConfigProcesses",
				"greedy": false,
				"target": ["ConfigProcesses"]
			}, {
				"name": "Home",
				"pattern": "",
				"greedy": false,
				"target": ["Home"]
			}],
			"config": {
				"viewPath": "fpe.view",
				"controlId": "PEHome",
				"controlAggregation": "pages"
			}
		}
	},
	"sap.platform.hcp": {
		"uri": "webapp",
		"_version": "1.1.0"
	}
}